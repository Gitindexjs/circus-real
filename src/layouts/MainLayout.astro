---
import BaseLayout from "../layouts/BaseLayout.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <Fragment slot="head">
    <title>{title}</title>
    <slot name="head" />
  </Fragment>
  <main class="text-center pt-8">
    <slot />
  </main>
</BaseLayout>
